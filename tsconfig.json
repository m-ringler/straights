{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ES2020",
    "lib": ["DOM", "ES2020"],
    "allowJs": false,
    "checkJs": false,
    "outDir": "",
    "rootDir": ".",
    "noEmitOnError": false,
    "noImplicitAny": false,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "Straights/**/html/**/*.ts",
    "Straights.Web/**/*.ts",
    "!**/node_modules/**"
  ]
}
