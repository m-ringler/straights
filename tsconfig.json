{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": ["DOM", "ES2022", "webworker"],
    "allowJs": false,
    "checkJs": false,
    "outDir": "",
    "rootDir": ".",
    "noEmitOnError": false,
    "noImplicitAny": false,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": false
  },
  "include": [
    "Straights/**/html/**/*.ts",
    "Straights.Web/**/*.ts"
  ]
}
