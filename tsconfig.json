{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": ["DOM", "ES2022", "webworker"],
    "allowJs": false,
    "checkJs": false,
    "outDir": "webdev/tsc-out",
    "rootDirs": ["webdev/ts/shared", "webdev/ts/webservice"],
    "noEmitOnError": false,
    "noImplicitAny": false,
    "alwaysStrict": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "verbatimModuleSyntax": true
  },
  "include": ["webdev/ts/**/*.ts"],
  "exclude": ["webdev/ts/**/*.test.ts", "webdev/ts/**/test/*.ts"]
}
