{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": ["DOM", "ES2022", "webworker"],
    "allowJs": false,
    "checkJs": false,
    "outDir": "",
    "rootDir": ".",
    "noEmitOnError": false,
    "noImplicitAny": false,
    "alwaysStrict": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "verbatimModuleSyntax": true
  },
  "include": ["./Straights/Play/html/*.ts", "./Straights.Web/*.ts"],
  "exclude": ["node_modules", "**/*.test.ts"]
}
